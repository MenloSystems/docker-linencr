<link rel="stylesheet" type="text/css" href="/static/basic.css" media="screen" />

<style>


#titel {
    width: 100%;
    background-color: grey;
}


</style>



<style>
    /* allgemeines */
    input.hackbox {
      display:none;
    }

    /* default-Zustand */
    span.volltext {
      display:none;
    }

    label.mehr_lesen {
      color:#0091D2;
      margin-left:200px;
    }

    label.mehr_lesen::before {
      content:"Einblenden: ";
      color:#0091D2;
    }

    /* bei gesetztem Haken */
    input.mehr_lesen:checked ~ span.volltext {
      display:inline;
    }

    input.mehr_lesen:checked ~ label.mehr_lesen::before {
      content:"Ausblenden: ";
      color:#0091D2;
    }
</style>


<div id="titel">
